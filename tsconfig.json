{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@templates/*": ["src/templates/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@contents/*": ["src/contents/*"]
    }
  },
  "exclude": [
    "../node_modules/",
    "/node_modules/",
    "../dist/**",    
    ]
}